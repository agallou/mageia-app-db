{
    "name"        : "mageia-app-db/mageia-app-db",
    "description" : "A social package browser",
    "license"     : "AGPL-3.0",
    "require": {
        "local/sf-guard-plugin"       : "v4.0.2",
        "local/om-cross-app-url-plugin" : "v1.0.2",
        "propel/sf-propel-o-r-m-plugin"       : "dev-master",
        "openlss/func-format" : "0.0.8",
        "natxet/cssmin": "3.0.x",
        "codegyre/robo": "^0.7",
        "leafo/scssphp": "0.2.0",
        "patchwork/jsqueeze": "2.0.x",
        "beelab/bowerphp": "*",
        "agallou/robo-hash": "^0.0.1",
        "henrikbjorn/lurker": "1.2.0",
        "friendsofsymfony1/symfony1": "^1.5"
},
    "require-dev": {
        "atoum/sfatoumplugin": "dev-master",
        "atoum/atoum"        : "dev-master",
        "local/el-x-h-prof-plugin"     : "1.0.0"
    },
    "scripts": {
        "post-install-cmd": [
          "./bin/robo install"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "allow-plugins": {
            "composer/installers": true,
            "samsonasik/package-versions": true
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/flint/Lurker"
        },
        {
            "type": "package",
            "package": {
                "version": "v1.4.20",
                "name": "symfony/symfony1",
                "dist": {
                    "url": "https://github.com/symfony/symfony1/archive/v1.4.20.zip",
                    "type": "zip"
                 }
            }
        },
       {
            "type": "package",
            "package": {
                "name": "local/sf-guard-plugin",
                "type" : "symfony1-plugin",
                "require" : {
                  "composer/installers":"*"
                },
                "version": "4.0.2",
                "source":  {
                    "url": "https://github.com/rdavaillaud/sfGuardPlugin-1.3.git",
                    "type": "git",
                    "reference": "5cb6c41cacd99436d1152faa459011e3dbd6ee66"
                }
            }
        },
       {
            "type": "package",
            "package": {
                "name": "local/om-cross-app-url-plugin",
                "type" : "symfony1-plugin",
                "require" : {
                  "composer/installers":"*"
                },
                "version": "1.0.2",
                "source":  {
                    "url": "https://github.com/Symfony-Plugins/omCrossAppUrlPlugin.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "local/el-x-h-prof-plugin",
                "type" : "symfony1-plugin",
                "require" : {
                  "composer/installers":"*"
                },
                "version": "1.0.0",
                "source":  {
                    "url": "https://github.com/Symfony-Plugins/elXHProfPlugin.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ]
}
